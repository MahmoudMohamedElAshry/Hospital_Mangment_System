@model ReceptionistViewModel
@{
    ViewData["Title"] = "Delete";
}

<div class=" about-section mt-5 container text-center">
    <h2 class="mb-4">Are You Sure ?</h2>
    <form asp-controller="Receptionist" asp-action="Delete">
        <div class="container py-2 text-center">
            <div asp-validation-summary="ModelOnly"></div>
             <partial name="PartialReceptionist\DeleteAndDetailsReceptionist">
            <div class="mt-4">
                <input type="submit" value="Delete" class="btn btn-outline-danger" />
                 <a asp-action="Index" class="btn btn-outline-dark">Cancel</a>
			</div>
        </div>
    </form>
</div>

